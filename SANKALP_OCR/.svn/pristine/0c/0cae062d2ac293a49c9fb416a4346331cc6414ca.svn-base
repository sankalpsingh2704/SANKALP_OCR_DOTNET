<script>


    $(function () {
        var availableTags = [];
        var availableIds = [];
        $.ajax({
            url: '/Invoice/getvendor/',
            dataType: 'json',
            success: function (months) {

                $.each(months, function (index, month) {

                    availableTags.push(month.text);

                });

            }
        });

        $("#drpvendorid").autocomplete({
            source: availableTags,
            response: function (event, ui) {
                if (!ui.content.length) {
                    var noResult = { value: "", label: "No Vendor matching your request" };
                    ui.content.push(noResult);
                }
            },
            select: function (event, ui) {
                $("#drpvendorid").val(ui.item.label);
                var vendorname = ui.item.value;
               

                $.post("getGSTIN",{
                    vendorname
                },function(data,status){
                
                    
                    $("#GSTIN").val(data.gstin);
                    
                });
            },
            minLength: 1
        });
    });
    function dateFormatter(dt){
        if(dt !=  ''){
            var num = dt.match(/\d+/g);
            var date = new Date(parseFloat(num)); //converting to date
            var day = date.getDate() ;
            var mnt = date.getMonth() + 1;
            var yr = date.getFullYear();
            var len1 = day.toString().length;
            var len2 = mnt.toString().length;
            var len3 = yr.toString().length;
            if (len1 == 1) {
                day = '0' + day;
            }
            if (len2 == 1) {
                mnt = '0' + mnt;
            }
            if (len3 == 2) {
                yr = '20' + yr;
            }
            //   var date1= ( date.getDate() + '/' + date.getMonth() + 1 +"/" + date.getFullYear());
            //return (day + "/" + mnt + "/" + yr);
            return (day + "/" + mnt + "/" + yr);
            //return date;
        }
        else
            return "";
    }

